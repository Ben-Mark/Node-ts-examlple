{
  "name": "animality",
  "version": "1.0.0",
  "description": "opencicd budget service",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "tests": "NODE_ENV=dev NODE_NO_WARNINGS=1 LOG_LEVEL=info mocha --config=src/tests/setup/.mocharc.json",
    "local-dependencies": "npm-run-all --parallel port-forward-mongodb port-forward-mongo-express port-forward-update-server",
    "port-forward-mongodb": "kubectl port-forward service/develeforce-mdb-service 27017:27017 -n tekton-pipelines",
    "port-forward-update-server": "kubectl port-forward service/develeforce-update-server 8443:443 -n develeforce-infra",
    "port-forward-mongo-express": "kubectl port-forward service/mongo-express-service 8081:8081 -n tekton-pipelines",
    "start-local": "NODE_ENV=dev ts-node-dev src/app.ts",
    "build": "tsc"
  },
  "author": "ben mark",
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "ioredis": "^5.2.2",
    "js-base64": "^2.5.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongodb": "^4.4.1",
    "mysql2": "^3.4.0",
    "node-rest-client-promise": "^3.1.1",
    "node-schedule": "^2.1.0",
    "redis": "^4.2.0",
    "redis-clustr": "^1.7.0",
    "uuid": "^9.0.0",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "chai": "^4.3.7",
    "expect": "^29.0.2",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "properties-reader": "^2.2.0",
    "should": "^13.2.3",
    "supertest": "^6.2.4",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.4.3"
  }
}
